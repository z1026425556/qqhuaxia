<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pengcheng.dao.EternalMapper">

    <insert id="addOne" useGeneratedKeys="true" keyProperty="id" parameterType="com.pengcheng.domain.Eternal">
        insert into spirit(area_id,area_son_id,profession,category,category_son) values(${areaId},${areaSonId},#{profession},#{category},#{categorySon});
    </insert>

</mapper>